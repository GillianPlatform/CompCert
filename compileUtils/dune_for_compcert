(library
  (name compcert)
  (public_name compcert)
  (modules_without_implementation c dwarfTypes)
  (libraries menhirLib str unix)
  (flags -w -27-9))

(install
  (files compcert.ini)
  (section
  (site
    (compcert iniConfig))))
    
(generate_sites_module
  (module iniConfig)
  (sites compcert))